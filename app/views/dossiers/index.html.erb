<div class="container mt-5">
  <div class="row">
    <div class="col">
       <div class="table-responsive">
          <table class="table table-bordered">
            <thead>
              <tr>
                <th> CLIENT</th>
                <th>MISSION</th>
                <th>LOCALISATION</th>
              </tr>
            </thead>
            <tbody>
             <% @dossiers.each do |d| %>
             <tr>
              <td> <%= link_to d.nomClient, dossier_path(d), class:"text-decoration-none text-dark" %></td>
              <td> <%= link_to d.objetMission, dossier_path(d), class:"text-decoration-none text-dark" %></td>
              <td><%= link_to d.localisation, dossier_path(d), class:"text-decoration-none text-dark"%></td>
             </tr>
             <% end %>
            </tbody>
          </table>
        </div>
       </div>
    </div>
  </div>
